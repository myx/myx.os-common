# common: csh and tcsh

setenv EDITOR nano
setenv LOCALE en_US.UTF-8
setenv LANG en_US.UTF-8
setenv CLICOLOR 'YES'
setenv LSCOLORS 'ExGxFxdxCxDxDxhbadExEx'
setenv PROMPT '%{\033[0;32m%}%n@%M%{\033[1;31m%}:%c%{\033[0;32m%}%#%{\033[0m%} '
setenv INPUTRC /usr/local/share/myx.common/include/console/etc/inputrc

set prompt = '%{\033[0;32m%}%n@%M%{\033[1;31m%}:%c%{\033[0;32m%}%#%{\033[0m%} '
