#!/bin/sh

. /usr/local/share/myx.common/include/subr-universal.sh

echo "Removing completion for: $HOME"

InternReplaceLine "$HOME/.bashrc" " ### myx.common completion ###$" "# removed by myx.common remove/completion  ### myx.common completion ###"
InternReplaceLine "$HOME/.cshrc" " ### myx.common completion ###$" "# removed by myx.common remove/completion  ### myx.common completion ###"
