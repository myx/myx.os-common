#!/bin/sh

##### !!!    This script is universal for FreeBSD, Darwin, Ubuntu.    !!! #####

SetupBashCompletion(){
	exit 1
}

case "$0" in
	*/myx.common/bin/lib/setupBashCompletion) 
		if [ -z "$1" ] ; then
			echo "Syntax: myx.common lib/setupBashCompletion <utility_name> <script_folder>" >&2
			exit 1
		fi
		set -e
		SetupBashCompletion "$@"
	;;
esac
