#!/bin/sh

##### !!! THIS SCRIPT MUST BE OVERRIDEN IN OS-DEPENDENT IMPLEMENTATION !!! #####

GetWheelGroupName(){
	# `echo wheel`
	echo "ERROR: GetWheelGroupName must be re-implemented" >&2
	exit 1
}


case "$0" in
	*/myx.common/bin/os/getWheelGroupName) 
		if [ "$1" = "--help" ] ; then
			echo "Syntax: myx.common os/getWheelGroupName" >&2
			exit 1
		fi
		set -e
		GetWheelGroupName "$@"
	;;
esac
