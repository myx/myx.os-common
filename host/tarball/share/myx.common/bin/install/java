#!/bin/sh

##### !!!    This script is universal for FreeBSD, Darwin, Ubuntu.    !!! #####
##### !!! THIS SCRIPT MAY BE OVERRIDEN IN OS-DEPENDENT IMPLEMENTATION !!! #####

set -e

if [ "`type -t UserRequireRoot`" != "function" ] ; then
. "/usr/local/share/myx.common/bin/user/requireRoot"
fi
UserRequireRoot "install/java"

[ ! -z `which java` ] && echo "java is already installed, version: " && echo "`java -Xinternalversion || java -version`" && exit 0 

myx.common lib/installEnsurePackage java
